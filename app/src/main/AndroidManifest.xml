<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.petukhova.flynotes">

    <application
        android:name="com.petukhova.flynotes.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_paper"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.petukhova.flynotes.Activity.PinActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.petukhova.flynotes.Activity.NewNoteActivity" />
        <activity android:name="com.petukhova.flynotes.Activity.NotesActivity" />
        <activity android:name="com.petukhova.flynotes.Activity.SetupActivity" />
    </application>


</manifest>